    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="5Chords"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpPage"
            android:label="@string/title_activity_help_page" >
        </activity>
        <activity
            android:name=".Score"
            android:label="@string/title_activity_score_page" >
        </activity>
        <activity
            android:name=".StartPage"
            android:label="@string/title_activity_start_page"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".AboutPage"
            android:label="@string/title_activity_about_page" >
        </activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
